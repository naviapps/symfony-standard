{
    "name": "naviapps/symfony-standard",
    "license": "proprietary",
    "type": "project",
    "description": "Symfony Standard Application",
    "require": {
        "php": "^7.1.3",
        "craue/formflow-bundle": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "egeloen/ckeditor-bundle": "^6.0",
        "friendsofsymfony/user-bundle": "^2.0",
        "knplabs/knp-menu-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^2.6",
        "naviapps/fos-user-bundle": "@dev",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "^3.4",
        "symfony/expression-language": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-apcu": "^1.6",
        "symfony/security-bundle": "^3.4",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/yaml": "^3.4",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.8",
        "symfony/browser-kit": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/debug-bundle": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/phpunit-bridge": "^3.4",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^3.4",
        "symfony/web-server-bundle": "^3.4"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Naviapps\\": "src/Naviapps/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0888YEQWWR1ZASCXHNJEFYT",
            "allow-contrib": false
        },
        "symfony-public-dir": "public"
    }
}
